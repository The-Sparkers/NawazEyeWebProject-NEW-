
@{
    ViewBag.Title = "ViewStore";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-md-offset-2 col-md-4">
            <div class="ViewSunglasses" onclick="goToSunglasses()">
                <header>
                    <div class="col-md-3 ViewSPGlassesBtn col-md-offset-5">
                        <h6><a href="@Url.Action("Index","Sunglasses")">View Sunglasses</a></h6>
                    </div>
                </header>
            </div>
        </div>
        <div class="col-md-4">
            <div class="ViewFrames" onclick="goToFrames()">
                <header>
                    <div class="col-md-4 ViewSPGlassesBtn col-md-offset-8">
                        <h6><a href="@Url.Action("Index","Frames")">View Frames</a></h6>
                    </div>
                </header>
            </div>
        </div>
    </div>
</div>

